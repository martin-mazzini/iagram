  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js",
    "debug": "nodemon --inspect=9229 app.js",
    "db:up": "docker-compose up -d",
    "db:down": "docker-compose down",
    "db:admin": "set DYNAMO_ENDPOINT=http://localhost:8000 && dynamodb-admin -p 8001",
    "dev:local": "npm run db:up && concurrently \"npm run db:admin\" \"nodemon app.js\"",
    "debug:local": "npm run db:up && concurrently \"npm run db:admin\" \"nodemon --inspect=9229 app.js"
  },
